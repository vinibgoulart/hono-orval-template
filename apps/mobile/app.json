{
  "expo": {
    "name": "Crypto Alert",
    "slug": "crypto-alert",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E3E59"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "crypto-alert"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png",
        "backgroundColor": "#1E3E59"
      }
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Poppins-Regular.otf"
          ]
        }
      ],
      "expo-localization"
    ]
  }
}